<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="cq:Page">
    <jcr:content
        cq:cloudservicename="testandtarget"
        cq:lastModifiedBy="admin"
        cq:template="/libs/cq/analytics/templates/testandtarget"
        jcr:lastModifiedBy="admin"
        jcr:primaryType="cq:PageContent"
        jcr:title="summit2017-target-config"
        sling:resourceType="cq/analytics/components/testandtargetpage"
        accountOptions="\{}"
        apiKind="REST"
        connectedWhen="email"
        email="summitlabuser2017@adobe.com"
        recommendationsEnabled="false">
        <recommendations
            jcr:primaryType="nt:unstructured"
            enableFeed="{Boolean}false"
            overrideExternalizer="{Boolean}false"
            recsFeedId="-1"/>
        <public
            jcr:primaryType="nt:unstructured"
            accurateTargeting="{Boolean}false"
            clientcode="aemlabsummit2017"
            clientlib="mboxjs"
            useDtm="{Boolean}true"/>
        <segmentsPollConfig
            jcr:mixinTypes="[cq:PollConfig]"
            jcr:primaryType="nt:unstructured"
            enabled="true"
            interval="{Long}60"
            source="adobe-target-segments:/etc/cloudservices/testandtarget/summit2017-target-config"
            target="/etc/segmentation/adobe-target/aemlabsummit2017"/>
        <accountOptionsUpdatePollConfig
            jcr:mixinTypes="[cq:PollConfig]"
            jcr:primaryType="nt:unstructured"
            enabled="false"
            interval="{Long}86400"
            source="adobe-target-account-options:/etc/cloudservices/testandtarget/summit2017-target-config"
            target="/etc/cloudservices/testandtarget/summit2017-target-config"/>
    </jcr:content>
    <summit2017-target-framework/>
</jcr:root>
